{
  "name": "Ultimate Web Blocker",
  "description": "A better ultimate web blocker.",
  "version": "1.0.4",
  "manifest_version": 3,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": ["storage", "tabs", "tabGroups"],
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
